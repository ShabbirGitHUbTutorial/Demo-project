<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>UPRN_Only_Numeric</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISBLANK(SPEN_UPRN__c),ISNUMBER(SPEN_UPRN__c))</errorConditionFormula>
    <errorMessage>Only numeric values are allowed for UPRN</errorMessage>
</ValidationRule>
