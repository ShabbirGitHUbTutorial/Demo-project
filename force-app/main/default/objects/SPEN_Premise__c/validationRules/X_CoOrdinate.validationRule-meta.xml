<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>X_CoOrdinate</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISBLANK( SPEN_XCoOrdinate__c ),ISNUMBER( SPEN_XCoOrdinate__c ))</errorConditionFormula>
    <errorMessage>Only numeric values are allowed for X Co-Ordinate</errorMessage>
</ValidationRule>
