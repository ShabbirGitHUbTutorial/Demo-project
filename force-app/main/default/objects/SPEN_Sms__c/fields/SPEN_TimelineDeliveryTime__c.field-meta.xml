<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SPEN_TimelineDeliveryTime__c</fullName>
    <description>Conditional formula field to populate either createddate or delivery time</description>
    <formula>IF(AND(ISBLANK(SPEN_DeliveryTimeFormula__c), ISPICKVAL(SPEN_Status__c, &apos;Sent&apos;)),  CreatedDate , SPEN_DeliveryTimeFormula__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Timeline Delivery Time</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>DateTime</type>
</CustomField>
