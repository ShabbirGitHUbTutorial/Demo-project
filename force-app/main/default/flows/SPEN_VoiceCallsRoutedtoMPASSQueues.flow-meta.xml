    <decisions>
        <name>DECISION_RouteToVoicemail</name>
        <label>Route to Voicemail?</label>
        <locationX>182</locationX>
        <locationY>674</locationY>
        <defaultConnector>
            <targetReference>INVOKE_GetMpasQueueId</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>DECISION_RouteToVoicemail_YES</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>INVOKE_AnAgentIsAvailable</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
