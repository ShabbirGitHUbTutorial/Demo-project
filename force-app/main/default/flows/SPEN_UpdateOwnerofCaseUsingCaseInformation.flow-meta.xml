        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Job_Status_Include_These_Values</name>
            <conditionLogic>(1 OR 2 OR 3 OR 4 OR 5) AND (6 OR 7) AND 8</conditionLogic>
            <conditions>
                <leftValueReference>$Record.SPEN_JobStatus__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Reject: Incomplete contact details</stringValue>
                </rightValue>
